{
  "name": "yz-yuki-plugin",
  "version": "2.0.6-0",
  "description": "优纪插件，yunzaijs 关于 微博推送、B站推送 等功能的拓展插件",
  "author": "snowtafir",
  "type": "module",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/snowtafir/yuki-plugin.git"
  },
  "scripts": {
    "app": "node lib/main.js",
    "dev": "lvy dev --yunzai",
    "build": "lvy build",
    "view": "lvy dev --view",
    "img-dev": "npx tsx watch --clear-screen=false jsxp.server.ts",
    "render-test": "npx tsx ./render.test.ts",
    "start": "pm2 startOrRestart pm2.config.cjs",
    "stop": "pm2 stop pm2.config.cjs",
    "delete": "pm2 delete pm2.config.cjs",
    "format": "prettier --write .",
    "check-format": "git diff --exit-code",
    "prepare": "husky"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "chalk": "^5.3.0",
    "chokidar": "4.0.1",
    "debug": "^4.3.6",
    "jsdom": "^25.0.1",
    "json5": "^2.2.3",
    "jsxp": "^1.0.4",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "moment": "^2.30.1",
    "node-fetch": "^3.3.2",
    "puppeteer": "^23.10.1",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "redis": "^4.7.0",
    "yaml": "^2.6.1",
    "yarn": "^1.19.1"
  },
  "devDependencies": {
    "@types/chalk": "2.2.0",
    "@types/chokidar": "2.1.7",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.7",
    "@types/md5": "^2.3.5",
    "@types/node": "^22.2.0",
    "@types/node-fetch": "^2.6.11",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/yaml": "1.9.7",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.9",
    "chokidar": "^4.0.1",
    "husky": "^9.1.6",
    "jsdom": "^24.1.1",
    "json5": "^2.2.3",
    "jsxp": "^1.0.4",
    "lodash": "^4.17.21",
    "lvyjs": "^0.2.6",
    "md5": "^2.3.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.47",
    "prettier": "^3.4.2",
    "puppeteer": "^23.10.1",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "redis": "^4.7.0",
    "tailwindcss": "^3.4.14",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "yaml": "^2.6.1",
    "yunzaijs": "^1.0.0-rc.5"
  },
  "files": [
    "public",
    "resources",
    "lib",
    "defaultConfig",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "LICENSE",
    ".puppeteerrc.cjs"
  ],
  "types": "lib",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "keywords": [
    "yunzaijs"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=16.14.0"
  }
}
